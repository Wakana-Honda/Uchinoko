<h1>ジャンル一覧・追加</h1>

<%= form_with model: @genre, url: '/genre', method: :post do |f| %>
 <%= f.label :ジャンル名 %>
 <%= f.text_field :name %>
 <%= f.submit '新規登録' %>
<% end %>


<div>ジャンル</div>

  <% @genres.each do |genre| %>
　　<%= genre.name %>
    <%= link_to "編集する", edit_genre_path(genre) %>
  <% end %>
  

